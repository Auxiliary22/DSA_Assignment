#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node* head = NULL;

void insert(int val) {
    Node* newNode = new Node{val, head};
    head = newNode;
}

int findMiddle() {
    Node* slow = head, *fast = head;
    while (fast && fast->next) {
        slow = slow->next;
        fast = fast->next->next;
    }
    return slow->data;
}

int main() {
    insert(5); insert(4); insert(3); insert(2); insert(1);
    cout << "Middle element: " << findMiddle() << endl;
}